<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/moodtracker"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alisa.starterhacks.MoodTracker">


    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="99dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="655dp"
        android:layout_height="752dp"
        app:srcCompat="@drawable/Frame"
        tools:layout_editor_absoluteX="-117dp"
        tools:layout_editor_absoluteY="-83dp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="194dp"
        android:layout_height="63dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:lineSpacingExtra="24sp"
        android:text="Mood Tracker"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="16dp" />

    <CalendarView
        android:id="@+id/calendarView2"
        android:layout_width="340dp"
        android:layout_height="302dp"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="131dp" />

    <ImageButton
        android:id="@+id/sadButtonId"
        android:layout_width="47dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/badbad"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="451dp" />

    <ImageButton
        android:id="@+id/happyButtonId"
        android:layout_width="43dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/goodgood"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="452dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="175dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="How are you feeling today?"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="456dp" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="27dp"
        android:layout_height="21dp"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="216dp" />

</android.support.constraint.ConstraintLayout>