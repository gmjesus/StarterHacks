<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/doctor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alisa.starterhacks.Doctor">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="678dp"
        android:layout_height="682dp"
        app:srcCompat="@drawable/Frame"
        tools:layout_editor_absoluteX="-146dp"
        tools:layout_editor_absoluteY="-11dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="324dp"
        android:layout_height="53dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:inputType="textPersonName"
        android:lineSpacingExtra="30sp"
        android:text="Doctor Information:"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        android:typeface="serif"
        android:visibility="visible"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="48dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="242dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="ADDRESS: 342 Haqers Street"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="308dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="265dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="PHONE NUMBER: 416 758 2093"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="251dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="147dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="NAME: Dr. Chang"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="194dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edit Info"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="400dp" />

</android.support.constraint.ConstraintLayout>